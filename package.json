{
    "name": "scheme",
    "displayName": "Scheme",
    "description": "Scheme language support for Visual Studio Code",
    "version": "0.0.1",
    "publisher": "rebornix",
    "author": {
		"name": "Peng Lv"
	},
	"contributors": [
		{
			"name": "Peng Lyu",
			"url": "https://github.com/rebornix"
		}
    ],
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "private": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/rebornix/vscode-scheme.git"
	},
	"bugs": {
		"url": "https://github.com/rebornix/vscode-scheme/issues"
	},
    "contributes": {
        "languages": [{
            "id": "scheme",
            "aliases": ["scheme", "scheme"],
            "extensions": [".scm",".sch",".rkt"],
            "configuration": "./scheme.configuration.json"
        }],
        "grammars": [{
            "language": "scheme",
            "scopeName": "source.scheme",
            "path": "./syntaxes/scheme.tmLanguage"
        }]
    }
}